{
  "name": "webdrivercss",
  "version": "0.1.0",
  "description": "Regression testing tool for WebdriverJS",
  "author": "Christian Bromann <mail@christian-bromann.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/jscoverage ./lib ./lib-cov --no-instrument lib/getPageInfo.js --no-instrument lib/makeScreenshot.js",
    "test": "./node_modules/.bin/mocha",
    "posttest": "WEBDRIVERCSS_COVERAGE=1 ./node_modules/.bin/mocha -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverjs/webdrivercss.git"
  },
  "dependencies": {
    "resemble": "^1.0.3",
    "async": "^0.2.10",
    "glob": "^3.2.9",
    "gm": "^1.14.2"
  },
  "devDependencies": {
    "mocha": "^1.18.2",
    "coveralls": "~2.10.0",
    "webdriverjs": "^1.5.1",
    "chai": "^1.9.1",
    "fs-extra": "^0.8.1",
    "mocha-lcov-reporter": "^0.0.1",
    "jscoverage": "^0.3.8"
  },
  "keywords": [
    "webdriverjs",
    "webdriver",
    "phantomcss",
    "regression",
    "test",
    "testing",
    "css",
    "responsive",
    "design"
  ]
}
